<launch>
  <arg name="machine" default="localhost"/>
  <arg name="port" default="/dev/ttyS3"/>
  <include file="$(find mav_startup)/machine/$(arg machine).machine" />
  <group ns="$(arg machine)" clear_params="true">
    <node machine="$(arg machine)" name="widemac_pub" pkg="sfly_widemac_com" type="widemac_pub" output="screen" respawn="true" >       
      <param name="port" value="$(arg port)" />  
    </node> 
  </group>
</launch>


